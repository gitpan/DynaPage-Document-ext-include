#!/usr/bin/perl -w

	use strict;
	use warnings;
	use Data::Dump qw(pp);
	use	DynaPage::Document;
	
	print "START\n";
	
	my	$opt	= 
	{
		RootDir			=> '/usr/local/src/perl-devel/DynaPage-Document/EXAMPLES',
		Document		=> '//usr/local/src/perl-devel/DynaPage-Document/EXAMPLES/document.dpd',
		
	};
	
	my	$dpd	= DynaPage::Document->new( $opt );
	
	print "PARSED\n";
	
	print pp($dpd);